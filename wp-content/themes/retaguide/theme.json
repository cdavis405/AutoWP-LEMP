{
  "$schema": "https://schemas.wp.org/wp/6.1/theme.json",
  "version": 2,
  "settings": {
    "color": {
      "palette": [
        { "slug": "light-blue", "name": "Light Blue", "color": "#E6F2FF" },
        { "slug": "teal", "name": "Teal", "color": "#4FB3BF" },
        { "slug": "light-green", "name": "Light Green", "color": "#E8F5E9" },
        { "slug": "dark-green", "name": "Dark Green", "color": "#2E7D32" },
        { "slug": "dark-gray", "name": "Dark Gray", "color": "#2B2B2B" },
        { "slug": "off-white", "name": "Off White", "color": "#FAFAFA" }
      ],
      "gradients": [],
      "duotone": [],
      "custom": false
    },
    "custom": {
      "retaguide": {
        "brand": {
          "primary": "#4FB3BF",
          "secondary": "#2E7D32"
        }
      }
    },
    "layout": {
      "contentSize": "720px",
      "wideSize": "1100px"
    },
    "spacing": {
      "units": [ "px", "em", "rem", "%" ]
    },
  "typography": {
      "fontFamilies": [
        {
          "fontFamily": "\"Source Sans Pro\", -apple-system, BlinkMacSystemFont, \"Segoe UI\", sans-serif",
          "slug": "source-sans-pro",
          "name": "Source Sans Pro"
        },
        {
          "fontFamily": "\"Merriweather\", Georgia, serif",
          "slug": "merriweather",
          "name": "Merriweather"
        }
      ],
      "fontSizes": [
        { "slug": "xs", "size": "0.875rem", "name": "XS" },
        { "slug": "sm", "size": "1rem", "name": "S" },
        { "slug": "base", "size": "1.125rem", "name": "Base" },
        { "slug": "lg", "size": "1.375rem", "name": "L" },
        { "slug": "xl", "size": "1.75rem", "name": "XL" },
        { "slug": "xxl", "size": "2.25rem", "name": "XXL" }
      ],
      "customFontSize": false
    },
    "appearanceTools": true,
    "blocks": {}
  },
  "styles": {
    "color": {
      "background": "#FAFAFA",
      "text": "#2B2B2B"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--source-sans-pro)"
    }
  },
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    }
  ]
}
